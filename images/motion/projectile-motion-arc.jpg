<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 200" style="background: #f0f9ff;">
  <!-- Background field -->
  <rect width="400" height="200" fill="#22c55e" opacity="0.2"/>
  
  <!-- Stadium outline -->
  <path d="M 0,180 Q 200,60 400,180" stroke="#374151" stroke-width="2" fill="none" opacity="0.3"/>
  
  <!-- Batter position -->
  <ellipse cx="50" cy="160" rx="8" ry="4" fill="#d4b084"/>
  <circle cx="50" cy="155" r="5" fill="#fbbf24"/>
  <text x="50" y="180" text-anchor="middle" font-size="10" fill="#374151">Batter</text>
  
  <!-- Parabolic trajectory -->
  <defs>
    <marker id="arrowhead" markerWidth="8" markerHeight="6" 
            refX="7" refY="3" orient="auto">
      <polygon points="0 0, 8 3, 0 6" fill="#dc2626"/>
    </marker>
  </defs>
  
  <!-- Main trajectory curve -->
  <path d="M 60,155 Q 200,50 350,130" 
        stroke="#dc2626" stroke-width="4" fill="none"
        marker-end="url(#arrowhead)" stroke-dasharray="6,3"/>
  
  <!-- Baseball at different positions along arc -->
  <circle cx="100" cy="115" r="3" fill="#ffffff" stroke="#dc2626" opacity="0.8"/>
  <circle cx="150" cy="85" r="3" fill="#ffffff" stroke="#dc2626" opacity="0.6"/>
  <circle cx="200" cy="75" r="3" fill="#ffffff" stroke="#dc2626" opacity="0.6"/>
  <circle cx="250" cy="85" r="3" fill="#ffffff" stroke="#dc2626" opacity="0.4"/>
  <circle cx="300" cy="105" r="3" fill="#ffffff" stroke="#dc2626" opacity="0.3"/>
  
  <!-- Velocity component vectors -->
  <g stroke="#3b82f6" stroke-width="2" fill="none">
    <!-- Horizontal components (constant) -->
    <line x1="100" y1="105" x2="120" y2="105" marker-end="url(#arrowhead)"/>
    <line x1="200" y1="65" x2="220" y2="65" marker-end="url(#arrowhead)"/>
    <line x1="300" y1="95" x2="320" y2="95" marker-end="url(#arrowhead)"/>
    
    <!-- Vertical components (changing) -->
    <line x1="110" y1="115" x2="110" y2="100" marker-end="url(#arrowhead)" opacity="0.8"/>
    <line x1="210" y1="75" x2="210" y2="65" marker-end="url(#arrowhead)" opacity="0.5"/>
    <line x1="310" y1="105" x2="310" y2="120" marker-end="url(#arrowhead)" opacity="0.8"/>
  </g>
  
  <!-- Velocity labels -->
  <text x="105" y="95" font-size="8" fill="#3b82f6">vₓ</text>
  <text x="90" y="108" font-size="8" fill="#3b82f6">vᵧ</text>
  
  <!-- Gravity arrow -->
  <g stroke="#ef4444" stroke-width="3">
    <line x1="30" y1="50" x2="30" y2="80" marker-end="url(#arrowhead)"/>
    <text x="35" y="70" font-size="10" fill="#ef4444">g</text>
  </g>
  
  <!-- Title -->
  <text x="200" y="25" text-anchor="middle" font-size="16" font-weight="bold" fill="#1f2937">
    Projectile Motion: Home Run Arc
  </text>
  <text x="200" y="40" text-anchor="middle" font-size="12" fill="#6b7280">
    Parabolic path under gravity's influence
  </text>
  
  <!-- Physics equations -->
  <text x="20" y="190" font-size="9" fill="#374151">
    x = v₀ₓt    y = v₀ᵧt - ½gt²    Range ≈ 400 feet
  </text>
  
</svg>